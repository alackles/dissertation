	keywords = {Fitness epistasis, Sign epistasis, Evolutionary genetics, Antibiotic resistance, Antibiotics, Cloning, Point mutation, Streptomycin},
	pages = {e1000578},
	file = {Full Text PDF:/home/acacia/Zotero/storage/V9PMEMXR/Trindade et al. - 2009 - Positive Epistasis Drives the Acquisition of Multi.pdf:application/pdf;Snapshot:/home/acacia/Zotero/storage/6D9UE8VH/article.html:text/html},
